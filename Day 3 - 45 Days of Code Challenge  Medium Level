#include <stdio.h>

int main() {
   int t; 
   scanf("%d",&t); 
   while(t--)
   {
       int x,y; 
       scanf("%d",&x); 
       scanf("%d",&y); 
       if(x%2==0||(x%2!=0&&y%2!=0))
       {
           printf("YES\n"); 
           
       } 
       else
       {
           printf("NO\n"); 
           
       }
   }
	return 0;
}
