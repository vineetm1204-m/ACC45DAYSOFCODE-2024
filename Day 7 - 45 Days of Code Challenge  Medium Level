#include <stdio.h>

int main() {
	 int V ;
	 scanf("%d",&V );
	 for(int i=0;i<V ;i++)
	 {
	   int N,A[1000000];
	   scanf("%d",&N);
	   int K;
	   scanf("%d",&K);
	   for(int j=0;j<N;j++)
	   {
	       scanf("%d",&A[j]);
	   }
	   
	   
	    for(int l=0;l<N;l++)
	   {
	       if(K>=A[l])
	       {
	           K-=A[l];
	           printf("1");
	       }
	       else
	       {
	           printf("0");
	       }
	   }  printf("\n"); 
	     
	   
	 }
	return 0;
}

